lm_PC3<-lm(pca_yields_df[2:4104,3] ~ pca_yields_df[1:4103,3])
lm_PC4<-lm(pca_yields_df[2:4104,4] ~ pca_yields_df[1:4103,4])
lm_PC5<-lm(pca_yields_df[2:4104,5] ~ pca_yields_df[1:4103,5])
